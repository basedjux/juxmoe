{
  "name": "juxmoe",
  "version": "1.0.0",
  "description": "My website, built in Next.js",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "xo && prettier",
    "css": "gulp css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basedjux/juxmoe.git"
  },
  "author": "Corbin Matschull",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basedjux/juxmoe/issues"
  },
  "homepage": "https://github.com/basedjux/juxmoe#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^4.2.0",
    "lodash": "^4.17.4",
    "next": "^3.0.1-beta.21",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "css-loader": "^0.28.4",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-xo-react": "^0.13.0",
    "eslint-plugin-react": "^7.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-watch": "^4.3.11",
    "lost": "^8.2.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "prettier": "^1.5.3",
    "pump": "^1.0.2",
    "style-loader": "^0.18.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "ignores": [
      "examples/**/*",
      "node_modules/**/*"
    ],
    "extends": "prettier",
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "no-unused-vars": 0,
      "import/order": 1,
      "no-warning-comments": 0,
      "prefer-promise-reject-errors": 0,
      "import/named": 1,
      "space-in-parens": 0,
      "object-curly-spacing": 0,
      "computed-property-spacing": 0,
      "space-infix-ops": 0,
      "one-var": 0,
      "no-console": 0,
      "semi": 0
    }
  }
}
